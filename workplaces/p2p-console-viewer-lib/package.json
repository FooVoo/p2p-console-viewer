{
  "name": "p2p-console-viewer-lib",
  "private": true,
  "version": "0.0.6",
  "type": "module",
  "main": "./src/public-api.js",
  "files": [
    "./src/**.js",
    "./src/**/*,js",
    "./types/**.d.ts"
  ],
  "types": "./types/public-api.d.ts",
  "scripts": {
    "dev": "vite --host",
    "build": "vite build",
    "type-gen": "npx -p typescript tsc src/*.js --declaration --allowJs --emitDeclarationOnly --outDir types",
    "prepack": "npm run type-gen",
    "preview": "vite preview",
    "postversion": "npm pack",
    "format": "prettier --write .",
    "lint": "prettier --check . && eslint ."
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "cors": "^2.8.5",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "express": "^5.2.1",
    "globals": "^16.5.0",
    "prettier": "^3.7.4",
    "uuid": "^13.0.0",
    "ws": "^8.18.3"
  }
}
